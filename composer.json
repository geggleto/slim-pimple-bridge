{
  "name": "geggleto/slim-pimple-bridge",
  "description": "Slim Pimple Bridge. Inject's slim's dependencies into a Pimple Container",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Glenn Eggleton",
      "email": "geggleto@gmail.com"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "Geggleto\\" : "src"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Geggleto\\Test\\" : "tests"
    }
  },
  "require": {
    "pimple/pimple": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "slim/slim": "3.0.0-RC3"
  }
}
